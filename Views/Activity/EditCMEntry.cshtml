											<option></option>
										</select>
										<span asp-validation-for="CommChannel" class="text-danger"></span>
									</div>


									<div class="form-group">
										<label asp-for="InOutBound" class="control-label"></label>
										<select class="js-select2 form-control" asp-for="InOutBound" asp-items="ViewBag.cmbCommType" style="width: 100%;" data-placeholder="Choose Comm Type">
											<option></option>
										</select>
										<span asp-validation-for="InOutBound" class="text-danger"></span>
									</div>
								</div>

								<div class="col-md-6">
									<div class="form-group">
										<label asp-for="Status" class="control-label"></label>
										<select class="js-select2 form-contro js-change-status" asp-for="Status" asp-items="ViewBag.cmbStatus" style="width: 100%;" data-placeholder="Choose Status">
											<option></option>
										</select>
										<span asp-validation-for="Status" class="text-danger"></span>
									</div>

									@*<div class="form-group">
											<label asp-for="SubStatus" class="control-label"></label>
											<select class="js-select2 form-control form-control-sm" asp-for="SubStatus" style="width: 100%;" data-placeholder="Choose Sub Status">
												<option></option>
											</select>
											<span asp-validation-for="SubStatus" class="text-danger"></span>
										</div>*@

									<div class="form-group">
										<label asp-for="Tagging" class="control-label"></label>
										<select class="js-select2 form-control js-change-tagging" asp-for="Tagging" asp-items="ViewBag.cmbClassification" style="width: 100%;" data-placeholder="Choose Classification">
											<option></option>
										</select>
										<span asp-validation-for="Tagging" class="text-danger"></span>
									</div>

									<div class="form-group">
										<label asp-for="Resolved" class="control-label"></label>
										<input asp-for="Resolved" class="form-control form-control-sm" data-week-start="1" data-autoclose="true" data-today-highlight="true" data-date-format="dd-M-yyyy" placeholder="Enter Due Date" autocomplete="off" />
										<span asp-validation-for="Resolved" class="text-danger"></span>
									</div>


								</div>

							</div>
						</div>
					</div>
				</div>

				@*Details of Concern/Activity*@
