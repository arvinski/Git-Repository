											<input asp-for="Location2" class="form-control form-control-sm" placeholder="Enter Location" autocomplete="off" id="location2" />
											<span asp-validation-for="Location2" class="text-danger"></span>
										}


									</div>

									<div class="form-group">
										<label asp-for="PaymentOf" class="control-label"></label>
										<select class="js-select2 form-control form-control-sm" asp-for="PaymentOf" asp-items="ViewBag.cmbPayInst" style="width: 100%;" data-placeholder="Choose Payment Institution">
											<option></option>
										</select>
										<span asp-validation-for="PaymentOf" class="text-danger"></span>
									</div>


								</div>

								<div class="col-md-3">

									<div class="form-group">
										<label asp-for="TerminalUsed" class="control-label"></label>
										<select class="js-select2 form-control form-control-sm" asp-for="TerminalUsed" asp-items="ViewBag.cmbTermUsed" style="width: 100%;" data-placeholder="Choose Terminal Used">
											<option></option>
										</select>
										<span asp-validation-for="TerminalUsed" class="text-danger"></span>
									</div>

									<div class="form-group">
										<label asp-for="BillerName" class="control-label"></label>
										<input asp-for="BillerName" class="form-control form-control-sm" placeholder="Enter Biller Name" autocomplete="off" />
										<span asp-validation-for="BillerName" class="text-danger"></span>
									</div>

									<div class="form-group">
										<label asp-for="Merchant" class="control-label"></label>
										<input asp-for="Merchant" class="form-control form-control-sm" placeholder="Enter Merchant / Website" autocomplete="off" />
										<span asp-validation-for="Merchant" class="text-danger"></span>
									</div>

									<div class="form-group row">
										<label asp-for="CardPresent" class="col-12"></label>
										<div class="col-12">
											<label class="css-control css-control-primary css-radio">
												<input type="radio" class="css-control-input" asp-for="CardPresent" id="cpPresent" value="Present">
												<span class="css-control-indicator"></span>Present
											</label>

											<label class="css-control css-control-primary css-radio">
												<input type="radio" class="css-control-input" asp-for="CardPresent" id="cpNotPresent" value="Not Present">
												<span class="css-control-indicator"></span>Not Present
											</label>

											<span asp-validation-for="CardPresent" class="text-danger"></span>
										</div>
									</div>

									<div class="form-group">
										<label asp-for="BancnetUsed" class="control-label"></label>
										<select class="js-select2 form-control" asp-for="BancnetUsed" asp-items="ViewBag.cmbBancUsed" style="width: 100%;" data-placeholder="Choose Bancnet ATM">
											<option></option>
										</select>
										<span asp-validation-for="BancnetUsed" class="text-danger"></span>
									</div>



									<div class="form-group">
										<label asp-for="Website" class="control-label"></label>
										<div class="custom-control custom-checkbox custom-control-inline mb-5 pull-right">
											<input class="custom-control-input" type="checkbox" asp-for="Online">
											<label class="custom-control-label" asp-for="Online"></label>
										</div>
										<input asp-for="Website" class="form-control form-control-sm" placeholder="Enter Website if online is checked" autocomplete="off" />
										<span asp-validation-for="Website" class="text-danger"></span>
									</div>


								</div>

								<div class="col-md-3">

									@*<div class="form-group">
											<label asp-for="Amount" class="control-label"></label>
											<input asp-for="Amount" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*?)\..*/g, '$1');" class="form-control form-control-sm" placeholder="Enter Amount" autocomplete="off" />
											<span asp-validation-for="Amount" class="text-danger"></span>
