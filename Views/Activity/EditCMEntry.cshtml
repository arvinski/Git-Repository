									</div>

									<div class="form-group">
										<label asp-for="Branch" class="control-label"></label>
										<select class="js-select2 form-control form-control-sm" asp-for="Branch" asp-items="ViewBag.cmbBranch" style="width: 100%;" data-placeholder="Choose Branch">
											<option></option>
										</select>
										<span asp-validation-for="Branch" class="text-danger"></span>
									</div>

								</div>

							</div>
						</div>
					</div>
				</div>

				@*SERVICE TICKET DETAILS*@
				<div class="block block-bordered block-rounded mb-2">
					<div class="block-header" role="tab" id="service_ticket_header">
