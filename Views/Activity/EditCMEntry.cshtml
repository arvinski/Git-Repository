											<option></option>
										</select>
										<span asp-validation-for="ATMTrans" class="text-danger"></span>
									</div>

									<div class="form-group">
										<label asp-for="ATMUsed" class="control-label"></label>
										@*<input asp-for="ATMUsed" class="form-control form-control-sm" placeholder="Enter ATM Used" autocomplete="off" />*@
										<select class="js-select2 form-control form-control-sm js-change-atmused" asp-for="ATMUsed" asp-items="ViewBag.cmbATMUsed" style="width: 100%;" data-placeholder="Choose ATM Used">
											<option></option>
										</select>
										<span asp-validation-for="ATMUsed" class="text-danger"></span>
									</div>


									<div class="form-group">
										<label asp-for="Location" class="control-label"></label>
										
										@if (ViewBag.isATMNga == "meron")
										{
											<select class="js-select2 form-control form-control-sm" asp-for="Location" asp-items="ViewBag.cmbLocation" style="width: 100%;" data-placeholder="Choose ATM Branch Location">
