@model AgentDesktop.Models.EditCallReport
@using AgentDesktop.Services
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
	ViewBag.Title = "Edit CM Entry";
}


<style>
	.footah {
		position: fixed;
		left: 0;
		bottom: 0;
		width: 100%;
		color: white;
		text-align: center;
	}

	.scroll-to-top {
		position: fixed;
		right: 1rem;
		bottom: 1rem;
		display: none;
		width: 2.75rem;
		height: 2.75rem;
		text-align: center;
		color: #fff;
		background: rgba(90, 92, 105, 0.5);
		line-height: 46px;
	}

		.scroll-to-top:focus, .scroll-to-top:hover {
			color: white;
		}

		.scroll-to-top:hover {
			background: #5a5c69;
		}

		.scroll-to-top i {
			font-weight: 800;
		}

	.rounded-sm {
		border-radius: 0.2rem !important;
	}
</style>

<form asp-action="EditCMEntry" method="post" enctype="multipart/form-data">
	<!-- Multiple Items -->
	<div class="block block-rounded block-themed">
		<div class="block-header bg-gd-earth">
			<h3 class="block-title">Customer Service Activity Edit for Ticket No. @ViewBag.TktNo</h3>
		</div>
		<div class="block-content">
			<input type="hidden" asp-for="RID">
			<input type="hidden" asp-for="TicketNo">
			<div id="cmactivity" role="tablist" aria-multiselectable="true">
				@*Client Information/Details*@
				<div class="block block-bordered block-rounded mb-2">
					<div class="block-header" role="tab" id="client_details_header">
						<a class="font-w600" data-toggle="collapse" data-parent="#cmactivity" href="#client_details" aria-expanded="true" aria-controls="client_details">
							CLIENT INFORMATION / DETAILS
						</a>
					</div>
					<div id="client_details" class="collapse show" role="tabpanel" aria-labelledby="client_details_header">
						<div class="block-content">
							<div class="row">
								<div class="col-md-6">

									<div class="form-group row">

										<div class="col-lg-12">
											<label class="control-label">Customer/Client Search</label>
											<div class="input-group">
												<input type="text" class="form-control form-control-sm" id="myInput" placeholder="Search Customer" autocomplete="off" />
												<div class="input-group-append">
													<span data-toggle="tooltip" data-placement="top" title="Search Customer via ICBA">
														<a id="btn_icba" href="#" + val data-target="#modal-container" data-toggle="modal" data-backdrop="static" data-keyboard="false" class="btn btn-sm btn-alt-primary">
															ICBA
														</a>
													</span>
												</div>
												<div class="input-group-append">
													<span data-toggle="tooltip" data-placement="top" title="Search Customer via BW">
														<a id="btn_bw" href="#" + val data-target="#modal-container" data-toggle="modal" data-backdrop="static" data-keyboard="false" class="btn btn-sm btn-alt-primary">
															BW
														</a>
													</span>
												</div>
											</div>
										</div>
									</div>


									<div class="form-group">
										<label asp-for="CustomerName" class="control-label"></label>
										<input asp-for="CustomerName" class="form-control form-control-sm" placeholder="Enter Customer Name" autocomplete="off" />
										<span asp-validation-for="CustomerName" class="text-danger"></span>
									</div>

								</div>

								<div class="col-md-6">
									<div class="form-group">
										<label asp-for="LastName" class="control-label"></label>
										<input asp-for="LastName" class="form-control form-control-sm" placeholder="Enter Last Name" autocomplete="off" />
										<span asp-validation-for="LastName" class="text-danger"></span>
									</div>
									<div class="form-group">
										<label asp-for="FirstName" class="control-label"></label>
										<input asp-for="FirstName" class="form-control form-control-sm" placeholder="Enter First Name" autocomplete="off" />

										<span asp-validation-for="FirstName" class="text-danger"></span>
									</div>
									<div class="form-group">
										<label asp-for="MiddleName" class="control-label"></label>
										<input asp-for="MiddleName" class="form-control form-control-sm" placeholder="Enter Middle Name if any leave it if none" autocomplete="off" />
										<span asp-validation-for="MiddleName" class="text-danger"></span>
									</div>

									@*<div class="form-group">
											<label asp-for="CardNo" class="control-label"></label>
											<div class="input-group">
												<input asp-for="CardNo" class="form-control form-control-sm" placeholder="Enter Card Number" autocomplete="off" value="@card_no" />
												<div class="input-group-append">
													<span data-toggle="tooltip" data-placement="top" title="Search Card Number">
														<a id="btn_card" href="#" + val data-target="#modal-card" data-toggle="modal" data-backdrop="static" data-keyboard="false" class="btn btn-sm btn-alt-primary">
															<i class="fa fa-search"></i>
														</a>
													</span>
												</div>
											</div>
											<span asp-validation-for="CardNo" class="text-danger"></span>
										</div>*@

									@*<div class="form-group">
											<label asp-for="CardNo" class="control-label"></label>
											<input asp-for="CardNo" class="form-control form-control-sm" placeholder="Enter Card Number" autocomplete="off" value="@card_no" />
											<span asp-validation-for="CardNo" class="text-danger"></span>
										</div>*@

									<div class="form-group row">
										<div class="col-lg-12">
											<label class="control-label">Card Number</label>
											<div class="input-group">
												<input asp-for="CardNo" class="form-control form-control-sm" placeholder="Enter Card Number" autocomplete="off" />
