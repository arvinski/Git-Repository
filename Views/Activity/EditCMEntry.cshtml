													<span data-toggle="tooltip" data-placement="top" title="Search Card Number">
														<a id="btn_card" href="#" + val data-target="#modal-container" data-toggle="modal" data-backdrop="static" data-keyboard="false" class="btn btn-sm btn-alt-primary">
															<i class="fa fa-search"></i>
														</a>
													</span>
												</div>
											</div>
											<span asp-validation-for="CardNo" class="text-danger"></span>
