																		case "R":
																			<span class="input-group-text">Renewed</span>
																			break;
																		case "W":
																			<span class="input-group-text">Mature Withdrawal</span>
																			break;

																		case "P":
																			<span class="input-group-text">Premature Withdrawal</span>
																			break;
																		case "E":
																			<span class="input-group-text">Error Out</span>
